{
  "env": {
    "browser": true,
    "es6": true,
    "es2017": true
  },
  "extends": ["airbnb-typescript", "prettier"],
  "rules": {
    "react/jsx-props-no-spreading": "off"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["*.test.tsx", "*.js"]
}
